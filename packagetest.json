{
    "name": "shemach",
    "author": "lemi diriba",
    "private": true,
    "license": "no",
    "scripts": {
        "dev": "cross-var $npm_execpath run development",
        "development": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-var $npm_execpath run development -- --watch",
        "watch-poll": "cross-var $npm_execpath run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "cross-var $npm_execpath run production",
        "production": "cross-env NODE_ENV=production webpack --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "test": "cross-env NODE_ENV=test jest",
        "tdd": "cross-var $npm_execpath run test -- --watch --notify"
    },
    "devDependencies": {
        "@coreui/coreui": "3.0.0-beta.3",
        "@coreui/coreui-chartjs": "2.0.0-beta.0",
        "@coreui/icons": "^1.0.0-beta.3",
        "chart.js": "^2.8.0",
        "laravel-mix": "^5.0.0",
        "pace-progress": "1.0.2",
        "perfect-scrollbar": "1.4.0",
        "@fortawesome/fontawesome-free": "^5.9.0",
        "@testing-library/dom": "^6.10.1",
        "@vue/test-utils": "^1.0.0-beta.29",
        "axios": "^0.19",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.8.0",
        "bootstrap": "^4.3.1",
        "cross-env": "^5.2.0",
        "cross-var": "^1.1.0",
        "jest": "^24.8.0",
        "jquery": "^3.4.1",
        "lodash": "^4.17.13",
        "pace": "github:HubSpot/pace#v1.0.2",
        "popper.js": "^1.15.0",
        "resolve-url-loader": "^2.3.2",
        "sass": "^1.21.0",
        "sass-loader": "^7.1.0",
        "sweetalert": "^2.1.2",
        "sweetalert2": "^8.19.0",
        "vue": "^2.6.10",
        "vue-jest": "^3.0.4",
        "vue-template-compiler": "^2.6.10"
    },
    "browserslist": {
        "development": "last 1 version",
        "production": ">2%, Firefox ESR"
    },
    "jest": {
        "testURL": "http://localhost",
        "roots": [
            "<rootDir>/tests/Javascript/"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/resources/js/$1"
        },
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            ".*\\.(vue)$": "vue-jest"
        }
    },
    "engines": {
        "node": ">=6.0.0"
    },
    "dependencies": {
        "bootstrap-sweetalert": "^1.0.1",
        "laravel-echo": "^1.6.1",
        "promise": "^8.0.3",
        "pusher-js": "^5.0.3",
        "vue-router": "^3.1.3",
        "vuetify": "^2.1.12"
    },
    "version": "1.0.0",
    "description": "shoping web",
    "main": "index.js",
    "repository": "git@github.com:lemidiriba/shemach.git"
}